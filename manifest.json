{
  "name": "Cherish",
  "version": "1",
  "description": "Selective memory for browser bookmarks",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "self",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  /* https://developer.chrome.com/extensions/commands
  "commands": {
	"toggle-ui": {
		"default": "Ctrl+D"
	}  
  }, */
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
